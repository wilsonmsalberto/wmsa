{
  "name": "wmsa",
  "description": "Wilson Alberto - Portfolio Site",
  "version": "1.0.0",
  "private": true,
  "author": "Wilson Alberto (https://wilsonalberto.com)",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "yarn start",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test:types": "tsc",
    "postinstall": "husky install",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:prettier:fix",
    "lint:eslint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint:eslint:fix": "yarn lint:eslint -- --fix",
    "lint:prettier": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --check",
    "lint:prettier:fix": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "format": "yarn lint:fix",
    "test": "jest",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "babel-plugin-styled-components": "2.1.4",
    "gatsby": "5.14.6",
    "gatsby-plugin-manifest": "5.13.1",
    "gatsby-plugin-robots-txt": "1.8.0",
    "gatsby-plugin-sharp": "5.13.1",
    "gatsby-plugin-styled-components": "6.13.1",
    "gatsby-source-filesystem": "5.13.1",
    "gatsby-transformer-sharp": "5.13.1",
    "polished": "4.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "styled-components": "6.1.13",
    "tsconfig-paths-webpack-plugin": "4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.25.7",
    "@babel/preset-react": "7.25.7",
    "@babel/preset-typescript": "7.25.7",
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/react-hooks": "8.0.1",
    "@types/jest": "29.5.13",
    "@types/node": "22.7.4",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "8.8.0",
    "@typescript-eslint/parser": "8.8.0",
    "babel-jest": "29.7.0",
    "babel-preset-gatsby": "3.13.2",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.8.3",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "husky": "9.1.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "typescript": "5.6.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
