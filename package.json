    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix",
    "lint:prettier": "prettier --check \"./src/**/*.{js,jsx,ts,css,html}\"",
    "prettier:fix": "prettier --write \"./src/**/*.{js,jsx,ts,css,html}\"",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "eslint-check": "eslint --print-config path/to/main.js | eslint-config-prettier-check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wilsonmsalberto/wmsa"
  },
  "dependencies": {
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
