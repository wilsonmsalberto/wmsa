    "gatsby-image": "2.4.21",
    "gatsby-plugin-manifest": "2.4.35",
    "gatsby-plugin-offline": "3.2.37",
    "gatsby-plugin-react-helmet": "3.3.14",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sharp": "2.6.43",
    "gatsby-plugin-svg-sprite": "2.0.1",
