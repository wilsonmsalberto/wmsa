{
  "name": "wmsa",
  "description": "Wilson Alberto - Portfolio Site",
  "version": "1.0.0",
  "private": true,
  "author": "Wilson Alberto (https://wilsonalberto.com)",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "yarn start",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test:types": "tsc",
    "postinstall": "husky install",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:prettier:fix",
    "lint:eslint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint:eslint:fix": "yarn lint:eslint -- --fix",
    "lint:prettier": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --check",
    "lint:prettier:fix": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "format": "yarn lint:fix",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "babel-plugin-styled-components": "2.1.4",
    "gatsby": "5.14.6",
    "gatsby-plugin-manifest": "5.14.0",
    "gatsby-plugin-robots-txt": "1.8.0",
    "gatsby-plugin-sharp": "5.14.0",
    "gatsby-plugin-styled-components": "6.14.0",
    "gatsby-plugin-typescript": "5.14.0",
    "gatsby-source-filesystem": "5.14.0",
    "gatsby-transformer-sharp": "5.14.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "styled-components": "6.1.19",
    "tsconfig-paths-webpack-plugin": "4.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.3",
    "@babel/preset-react": "7.27.1",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@types/node": "22.7.4",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "babel-preset-gatsby": "3.14.0",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.2",
    "prettier": "3.6.2",
    "typescript": "5.6.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
